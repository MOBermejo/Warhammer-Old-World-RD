  <sharedProfiles>
    
    <!-- ARMY SPECIAL RULES -->
    
    <profile name="Primal Fury" hidden="false" id="24a7-8ec2-fca1-21e2" typeId="c1ac-c1c8-f9d5-9673" typeName="Special Rule">
      <characteristics>
        <characteristic name="Description" typeId="9f84-4221-785a-db50">At the beginning of each round of close combat, each engaged unit with the Primal Fury special rule must take a Leadership test. If the test is passed, that unit gains the Hatred special rule until the end of the close combat phase. In addition, if they passed the Leadership test and rolled a double, they also gain the Frenzy special rule until the end of the close combat phase. A unit with this special rule may become Frenzied in this way even if it has lost Frenzy earlier in the game.</characteristic>
      </characteristics>
    </profile>

    <profile name="Bestial Charge" hidden="false" id="4203-7f82-8eaa-bfd3" typeId="c1ac-c1c8-f9d5-9673" typeName="Special Rule">
      <characteristics>
        <characteristic name="Description" typeId="9f84-4221-785a-db50">When charging, can re-roll one of the dice.</characteristic>
      </characteristics>
    </profile>

    <profile name="Beastmen Ambush" hidden="false" id="ba01-1234-5678-9abc" typeId="c1ac-c1c8-f9d5-9673" typeName="Special Rule">
      <characteristics>
        <characteristic name="Description" typeId="9f84-4221-785a-db50">All models with this rule have the Ambusher special rule. In addition, they add +1 to any rolls to determine whether or not they enter the table when using the Ambushers special rule.</characteristic>
      </characteristics>
    </profile>

    <profile name="Crushing Blows" hidden="false" id="cb01-9876-5432-1def" typeId="c1ac-c1c8-f9d5-9673" typeName="Special Rule">
      <characteristics>
        <characteristic name="Description" typeId="9f84-4221-785a-db50">Increase AP with any melee attack (including magical weapons, Stomps and Impact Hits) by 1 (From AP/ to AP-1, from AP-1 to AP-2, etc.).</characteristic>
      </characteristics>
    </profile>

    <profile name="Despoilers" hidden="false" id="des1-2345-6789-abcd" typeId="c1ac-c1c8-f9d5-9673" typeName="Special Rule">
      <characteristics>
        <characteristic name="Description" typeId="9f84-4221-785a-db50">Each enemy standard bearer that is removed as a result of a successful pursuit move or slain in combat by a unit of Bestigors adds +1 to their future Combat Resolution. If the unit flees for any reason, they will lose this bonus.</characteristic>
      </characteristics>
    </profile>

    <!-- MARKS OF CHAOS -->
    
    <profile name="Mark of Chaos Undivided" hidden="false" id="595-bbd0-34c7-e601" typeId="c1ac-c1c8-f9d5-9673" typeName="Special Rule">
      <characteristics>
        <characteristic name="Description" typeId="9f84-4221-785a-db50">Models with the Mark of Chaos Undivided can re-roll any failed Fear, Panic or Terror tests.</characteristic>
      </characteristics>
    </profile>

    <profile name="Mark of Tzeentch" hidden="false" id="mtz1-2345-6789-abcd" typeId="c1ac-c1c8-f9d5-9673" typeName="Special Rule">
      <characteristics>
        <characteristic name="Description" typeId="9f84-4221-785a-db50">Models with the Mark of Tzeentch have the Magic Resistance (-1) and Ward save (6+) special rules.</characteristic>
      </characteristics>
    </profile>

    <profile name="Mark of Nurgle" hidden="false" id="mnu1-2345-6789-abcd" typeId="c1ac-c1c8-f9d5-9673" typeName="Special Rule">
      <characteristics>
        <characteristic name="Description" typeId="9f84-4221-785a-db50">Models with the Mark of Nurgle have the Poisoned Attacks special rule and Enemy models in base contact suffer -1 to their Weapon Skill.</characteristic>
      </characteristics>
    </profile>

    <profile name="Mark of Khorne" hidden="false" id="mkh1-2345-6789-abcd" typeId="c1ac-c1c8-f9d5-9673" typeName="Special Rule">
      <characteristics>
        <characteristic name="Description" typeId="9f84-4221-785a-db50">Models with the Mark of Khorne have the Counter Charge and Furious Charge special rule.</characteristic>
      </characteristics>
    </profile>

    <profile name="Mark of Slaanesh" hidden="false" id="msl1-2345-6789-abcd" typeId="c1ac-c1c8-f9d5-9673" typeName="Special Rule">
      <characteristics>
        <characteristic name="Description" typeId="9f84-4221-785a-db50">Models with the Mark of Slaanesh have the Immune to Psychology special rule and +1 MVT.</characteristic>
      </characteristics>
    </profile>

    <!-- GIFTS OF CHAOS -->
    
    <profile name="Uncanny Senses" hidden="false" id="gift-uncanny-5pts" typeId="c1ac-c1c8-f9d5-9673" typeName="Special Rule">
      <characteristics>
        <characteristic name="Description" typeId="9f84-4221-785a-db50">Model gains +1I.</characteristic>
      </characteristics>
    </profile>

    <profile name="Gnarled Hides" hidden="false" id="gift-gnarled-5pts" typeId="c1ac-c1c8-f9d5-9673" typeName="Special Rule">
      <characteristics>
        <characteristic name="Description" typeId="9f84-4221-785a-db50">Model improves its armour save by 1.</characteristic>
      </characteristics>
    </profile>

    <profile name="Gouge-Tusks" hidden="false" id="gift-gouge-10pts" typeId="c1ac-c1c8-f9d5-9673" typeName="Special Rule">
      <characteristics>
        <characteristic name="Description" typeId="9f84-4221-785a-db50">Model gains the Impact Hits (1) special rule or gains +1 to their Impact Hits.</characteristic>
      </characteristics>
    </profile>

    <profile name="Many-Limbed Fiend" hidden="false" id="gift-manylimbed-15pts" typeId="c1ac-c1c8-f9d5-9673" typeName="Special Rule">
      <characteristics>
        <characteristic name="Description" typeId="9f84-4221-785a-db50">This model (but not its mount) has a +1 modifier to its Attacks characteristic. However, this bonus attack must be made using an ordinary Hand Weapon.</characteristic>
      </characteristics>
    </profile>

    <profile name="Rune of the Beast Ascendant" hidden="false" id="gift-rune-15pts" typeId="c1ac-c1c8-f9d5-9673" typeName="Special Rule">
      <characteristics>
        <characteristic name="Description" typeId="9f84-4221-785a-db50">Model gains +1LD.</characteristic>
      </characteristics>
    </profile>

    <profile name="Muscular Monstrosity" hidden="false" id="gift-muscular-20pts" typeId="c1ac-c1c8-f9d5-9673" typeName="Special Rule">
      <characteristics>
        <characteristic name="Description" typeId="9f84-4221-785a-db50">Model gains +1S.</characteristic>
      </characteristics>
    </profile>

    <profile name="Crown of Horns" hidden="false" id="gift-crown-20pts" typeId="c1ac-c1c8-f9d5-9673" typeName="Special Rule">
      <characteristics>
        <characteristic name="Description" typeId="9f84-4221-785a-db50">Characters only. This character and any unit they have joined gains the Stubborn special rule.</characteristic>
      </characteristics>
    </profile>

    <profile name="Pelt of Midnight" hidden="false" id="gift-pelt-25pts" typeId="c1ac-c1c8-f9d5-9673" typeName="Special Rule">
      <characteristics>
        <characteristic name="Description" typeId="9f84-4221-785a-db50">Characters only. Any enemy model that targets this character or any unit they have joined during The Shooting Phase suffers an additional -1 To Hit modifier.</characteristic>
      </characteristics>
    </profile>

    <!-- OTHER SPECIAL RULES -->
    
    <profile name="Braystaff" hidden="false" id="583e-832e-5e02-23cb" typeId="c1ac-c1c8-f9d5-9673" typeName="Special Rule">
      <characteristics>
        <characteristic name="Description" typeId="9f84-4221-785a-db50">When this model's combat is chosen during Step 1.1 of any Choose &amp; Fight Combat sub-phase, it must choose to use its Braystaff offensively or defensively. Used offensively, a Braystaff counts as a great weapon. Used defensively, a Braystaff counts as a hand weapon and gives its wielder an Armour Value of 5+.</characteristic>
      </characteristics>
    </profile>

    <profile name="Razor Tusks" hidden="false" id="3650-49a2-85b-702e" typeId="c1ac-c1c8-f9d5-9673" typeName="Special Rule">
      <characteristics>
        <characteristic name="Description" typeId="9f84-4221-785a-db50">During a Turn in which it charged, the Armour Piercing characteristic of a Tuskgor's or Razorgor's tusks Attacks and Impact Hits is AP-1.</characteristic>
      </characteristics>
    </profile>

    <profile name="Drunken" hidden="false" id="7c-7525-760e-17ad" typeId="c1ac-c1c8-f9d5-9673" typeName="Special Rule">
      <characteristics>
        <characteristic name="Description" typeId="9f84-4221-785a-db50">At the start of each strategy phase, roll a D6 for this model or the unit they joined and consult the table below to see what effect they have until the start of their next turn: 1-2, Drunken Stupor: The unit gains the Stupidity special rule. 3-4, Drunken Frenzy: The unit gains the Furious Charge special rule. 5-6, Drunken Bravado: The unit gains the Stubborn special rule.</characteristic>
      </characteristics>
    </profile>

    <profile name="Ensorcelled Weapons" hidden="false" id="ensw-1111-2222-3333" typeId="c1ac-c1c8-f9d5-9673" typeName="Special Rule">
      <characteristics>
        <characteristic name="Description" typeId="9f84-4221-785a-db50">Applies to all melee weapons.</characteristic>
      </characteristics>
    </profile>

    <profile name="Maddening Aura" hidden="false" id="madd-1234-5678-90ab" typeId="c1ac-c1c8-f9d5-9673" typeName="Special Rule">
      <characteristics>
        <characteristic name="Description" typeId="9f84-4221-785a-db50">Each enemy unit within 12" of one or more Jabberslythes at the beginning of the Beastmen Magic phase must take a Leadership test. For every point by which a unit fails its test, it suffers a wound which Ignores Armour Saves. This has no effect on units with Immunity (Psychology).</characteristic>
      </characteristics>
    </profile>

    <profile name="Petrifying Gaze" hidden="false" id="petr-1234-5678-90ab" typeId="c1ac-c1c8-f9d5-9673" typeName="Special Rule">
      <characteristics>
        <characteristic name="Description" typeId="9f84-4221-785a-db50">Auto-Hit, that forces an enemy to take an Initiative test or suffer a wound with no armour saves allowed and with the Multiple Wounds (D3) and Magical Attacks special rule.</characteristic>
      </characteristics>
    </profile>

    <profile name="Stony Stare" hidden="false" id="ston-1234-5678-90ab" typeId="c1ac-c1c8-f9d5-9673" typeName="Special Rule">
      <characteristics>
        <characteristic name="Description" typeId="9f84-4221-785a-db50">If this test is failed, they suffer D3 wounds with no armour saves permitted.</characteristic>
      </characteristics>
    </profile>

    <profile name="Ghosight" hidden="false" id="ghos-1234-5678-90ab" typeId="c1ac-c1c8-f9d5-9673" typeName="Special Rule">
      <characteristics>
        <characteristic name="Description" typeId="9f84-4221-785a-db50">Does not affect enemy models with regeneration save.</characteristic>
      </characteristics>
    </profile>

    <profile name="Soul-Eater" hidden="false" id="soul-1234-5678-90ab" typeId="c1ac-c1c8-f9d5-9673" typeName="Special Rule">
      <characteristics>
        <characteristic name="Description" typeId="9f84-4221-785a-db50">Has 24" range.</characteristic>
      </characteristics>
    </profile>

    <profile name="Crown of Antlers" hidden="false" id="crow-1234-5678-90ab" typeId="c1ac-c1c8-f9d5-9673" typeName="Special Rule">
      <characteristics>
        <characteristic name="Description" typeId="9f84-4221-785a-db50">Can re-roll the number of Impact Hits.</characteristic>
      </characteristics>
    </profile>

    <profile name="Troll Vomit" hidden="false" id="trlv-1111-2222-3333" typeId="c1ac-c1c8-f9d5-9673" typeName="Special Rule">
      <characteristics>
        <characteristic name="Description" typeId="9f84-4221-785a-db50">Troll Vomit is S5 instead of S3.</characteristic>
      </characteristics>
    </profile>

    <profile name="Acidic Vomit" hidden="false" id="acid-1234-5678-90ab" typeId="c1ac-c1c8-f9d5-9673" typeName="Special Rule">
      <characteristics>
        <characteristic name="Description" typeId="9f84-4221-785a-db50">S4, AP-1 Breath Weapon.</characteristic>
      </characteristics>
    </profile>

