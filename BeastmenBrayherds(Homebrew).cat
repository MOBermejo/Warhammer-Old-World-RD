    <!-- Lore of Beasts Spells -->
    <profile name="Primal Onslaught" hidden="false" id="spell-primal-onslaught" typeId="c1ac-c1c8-f9d5-9673" typeName="Special Rule">
      <characteristics>
        <characteristic name="Description" typeId="9f84-4221-785a-db50">Enchantment, Casting Value 5+/7+, Range 12". Target/all friendly units can re-roll their Primal Fury tests.</characteristic>
      </characteristics>
    </profile>

    <profile name="Bestial Surge" hidden="false" id="spell-bestial-surge" typeId="c1ac-c1c8-f9d5-9673" typeName="Special Rule">
      <characteristics>
        <characteristic name="Description" typeId="9f84-4221-785a-db50">Enchantment, Casting Value 6+/8+, Range 12". Target/all friendly units may immediately move forward 2D6 (roll separately for each).</characteristic>
      </characteristics>
    </profile>

    <profile name="Devolve" hidden="false" id="spell-devolve" typeId="c1ac-c1c8-f9d5-9673" typeName="Special Rule">
      <characteristics>
        <characteristic name="Description" typeId="9f84-4221-785a-db50">Hex, Casting Value 8+. The target enemy unit immediately make a Leadership test. If this test is failed, it loses a number of Wounds equal to the amount by which if failed the test. Cavalry, Monstrous Cavalry, Monstrous Creatures and Behemoths that are not Immune to Psychology, roll an additional D6 and discard the lowest.</characteristic>
      </characteristics>
    </profile>

    <profile name="Viletide" hidden="false" id="spell-viletide" typeId="c1ac-c1c8-f9d5-9673" typeName="Special Rule">
      <characteristics>
        <characteristic name="Description" typeId="9f84-4221-785a-db50">Direct Damage, Casting Value 10+/12+, Range 18". Target/all enemy units suffer D6 Strength 2 hits with the Poisoned Attacks special rule.</characteristic>
      </characteristics>
    </profile>

    <profile name="Mantle of Ghorgok" hidden="false" id="spell-mantle-ghorgok" typeId="c1ac-c1c8-f9d5-9673" typeName="Special Rule">
      <characteristics>
        <characteristic name="Description" typeId="9f84-4221-785a-db50">Enchantment, Casting Value 12+, Range 12". Target friendly unit gains +1 Strength and Toughness. Additionally, all models in the unit gain the Regeneration (6+) special rule.</characteristic>
      </characteristics>
    </profile>

    <profile name="Lore of Beasts" hidden="false" id="6419-d95f-555e-16b9" typeId="c1ac-c1c8-f9d5-9673" typeName="Special Rule">
      <characteristics>
        <characteristic name="Description" typeId="9f84-4221-785a-db50">A Wizard with the 'Lore of Beasts' special rule may discard one of their randomly generated spells as normal. When they do so, they may select instead the signature spell of their chose Lore of Magic, or one of the following spells: Viletide, Devolve, Mantle of Ghorgok</characteristic>
      </characteristics>
    </profile>

    <!-- Magic Item Profiles -->
    <profile name="Primeval Club" hidden="false" id="primeval-club-profile" typeId="e4d1-790a-ab75-2b57" typeName="Magic Weapons">
      <characteristics>
        <characteristic name="R" typeId="51c8-c0d6-94db-264c">Combat</characteristic>
        <characteristic name="S" typeId="4f75-25c9-31bd-7074">+3</characteristic>
        <characteristic name="AP" typeId="15e2-fff4-477f-ea26">-3</characteristic>
