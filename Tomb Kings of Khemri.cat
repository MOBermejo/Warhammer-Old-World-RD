    <profile name="Necrotect" hidden="false" id="7e83-1424-1267-5a9" typeId="b070-143a-73f-2772" typeName="Model">
      <characteristics>
        <characteristic name="M" typeId="cd3b-a5a4-e185-5a9d">4</characteristic>
